   "metadata": {},
   "source": [
    "Ecrivez un programme NumPy pour convertir un tableau NumPy en une structure de liste Python."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "dd899f12",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "f6763093",
   "metadata": {},
   "outputs": [],
   "source": [
    "my_array = np.array([[1,2],[3,4],[5,6]])#Tableau numpy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "1efe9259",
   "metadata": {},
   "outputs": [],
   "source": [
    "def Array_To_List(array):\n",
    "    return array.tolist()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "8bbe8846",
   "metadata": {},
   "outputs": [],
   "source": [
    "my_list = Array_To_List(my_array)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "6506c805",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1, 2], [3, 4], [5, 6]]\n"
     ]
    }
   ],
   "source": [
    "print(my_list)"
   ]
  }
