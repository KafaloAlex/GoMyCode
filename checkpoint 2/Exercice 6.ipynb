   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "64276af9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[ 1. -1.]\n",
      " [-1.  1.]]\n"
     ]
    }
   ],
   "source": [
    "def Covariance(matrix):\n",
    "    result = np.cov(matrix)\n",
    "    return print(result)\n",
    "\n",
    "m1 = np.array([[0, 1, 2], [2, 1, 0]])\n",
    "\n",
    "if(len(m1[0]) == len(m1[1])):\n",
    "    Covariance(m1)\n",
    "else:\n",
    "    print(\"Les deux tableaux doivent avoir la même dimension\")"
   ]
